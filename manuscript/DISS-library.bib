  shorttitle = {Blunted {{Neural Responses}} to {{Reward}} in {{Remitted Major Depression}}},
  author = {Whitton, Alexis E. and Kakani, Pragya and Foti, Dan and Van't Veer, Ashlee and Haile, Anja and Crowley, David J. and Pizzagalli, Diego A.},
  year = {2016},
