@article{bondyEmotionProcessingFemale2018,
  title = {Emotion Processing in Female Youth: {{Testing}} the Stability of the Late Positive Potential},
  shorttitle = {Emotion Processing in Female Youth},
  author = {Bondy, Erin and Stewart, Jeremy G. and Hajcak, Greg and Weinberg, Anna and Tarlow, Naomi and Mittal, Vijay A. and Auerbach, Randy P.},
  year = {2018},
  month = feb,
