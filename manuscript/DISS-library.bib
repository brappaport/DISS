@article{dugreLossAnticipationOutcome2018,
  title = {Loss Anticipation and Outcome during the {{{\emph{Monetary Incentive Delay Task}}}} : A Neuroimaging Systematic Review and Meta-Analysis},
  shorttitle = {Loss Anticipation and Outcome during the {{{\emph{Monetary Incentive Delay Task}}}}},
