  volume = {68},
  pages = {154--165},
  issn = {0004-9530},
  doi = {10.1111/ajpy.12130},
  abstract = {Objectives: Social anxiety disorder involves biased cognition and altered neural responses to social stimuli. This study further assesses the precise ways in which neural activation associated with perceptual processing of faces differs in socially anxious and non-anxious individuals. Method: Functional magnetic resonance imaging data were acquired as 90 anxious or healthy juveniles and adults performed a peer feedback task. Psychophysiological functional connectivity analysis was performed on all participants using a seed placed in the right fusiform face area (rFFA). Results: Social anxiety was associated with enhanced rFFA coupling in several areas of ventral visual stream when viewing feedback from peers previously rejected by participants; healthy participants demonstrated the opposite pattern. Moreover, anxious juveniles had greater positive rFFA coupling with right inferior temporal gyrus during feedback from rejected peers; other groups showed the opposite pattern. Finally, anxious juveniles had greater positive rFFA coupling with pregenual anterior cingulate cortex during negative peer feedback; other groups displayed the opposite pattern. Conclusions: Anxious individuals, and juveniles in particular, may dedicate more neural resources to stimuli associated with potentially negative, relative to positive, social outcomes; non-anxious individuals may do the opposite. (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
  file = {/Users/brentrappaport/Zotero/storage/SIAE39JC/Beer et al. - 2016 - Anxiously elaborating the social percept Anxiety .pdf},
  journal = {Australian Journal of Psychology},
  keywords = {Age Differences,Brain Connectivity,Brain Development,development,face perception,Face Perception,Fusiform Gyrus,Perceptual Development,psychophysiological interaction,Psychophysiology,social anxiety,Social Anxiety},
  number = {3}
}

@article{beldenNeuralActivationAssociated2014,
  title = {Neural Activation Associated with the Cognitive Emotion Regulation of Sadness in Healthy Children},
